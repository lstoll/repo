[user]
	name = Lincoln Stoll
	email = lincoln.stoll@gmail.com
[color]
	diff = auto
	status = auto
	branch = auto
	ui = true
[alias]
  st = status --short --branch
  co = checkout
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  lp = log --patch
  dc = diff --cached
  ack = grep --break --heading --line-number

[github]
	user = lstoll

[include]
  # For username / creds / etc
  path = ~/.gitconfig.local
[push]
	default = simple
[credential]
	helper = osxkeychain
[color "grep"]
	filename = bold green
	match = black yellow
[url "git@github.com:"]
	insteadOf = https://github.com/
